<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="restservice.infrastructure.datasource.event.EventMapper">

    <resultMap id="EventDataList" type="restservice.infrastructure.datasource.event.EventData">
        <result property="id" column="ID"/>
        <result property="event_date_time" column="EVENT_DATE_TIME"/>
    </resultMap>

    <select id="selectEventData" resultMap="EventDataList">
        SELECT
            ID
            ,EVENT_DATE_TIME
        FROM
            TIME_EVENT;
    </select>

</mapper>